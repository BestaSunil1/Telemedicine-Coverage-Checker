package training.iqgateway.entities;

import org.springframework.data.annotation.Id;
import org.springframework.data.mongodb.core.mapping.DBRef;
import org.springframework.data.mongodb.core.mapping.Document;

@Document(collection  = "patients")
public class Patient {
	@Id
	private String id;
	@DBRef
	private User user;
	private String dateOfBirth;
	private String gender;
	@DBRef
	private	InsurenceEligibilities insurence; 
	private String contactNumber;
	
	public Patient() {
		
	}

	public Patient(String id, User user, String dateOfBirth, String gender, InsurenceEligibilities insurence, String contactNumber) {
		super();
		this.id = id;
		this.user = user;
		this.dateOfBirth = dateOfBirth;
		this.gender = gender;
		this.insurence = insurence;
		this.contactNumber = contactNumber;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public User getUser() {
		return user;
	}

	public void setUser(User user) {
		this.user = user;
	}

	public String getDateOfBirth() {
		return dateOfBirth;
	}

	public void setDateOfBirth(String dateOfBirth) {
		this.dateOfBirth = dateOfBirth;
	}

	public String getGender() {
		return gender;
	}

	public void setGender(String gender) {
		this.gender = gender;
	}

	public InsurenceEligibilities getInsurence() {
		return insurence;
	}

	public void setInsurence(InsurenceEligibilities insurence) {
		this.insurence = insurence;
	}

	public String getContactNumber() {
		return contactNumber;
	}

	public void setContactNumber(String contactNumber) {
		this.contactNumber = contactNumber;
	}
	
	
	
}
