package training.iqgateway.dtos;

import java.time.LocalDateTime;

import training.iqgateway.entities.EligibilityStatus;

public class PatientInsuranceDTO {
    private String eligibilityId;
    private String planName;
    private String planProvider;
    private String planType;
    private Integer planCost;
    private EligibilityStatus status;
    private String benefitDetails;
    private LocalDateTime verifiedAt;
    
    public PatientInsuranceDTO() {
	}

	public PatientInsuranceDTO(String eligibilityId, String planName, String planProvider, String planType,
			Integer planCost, EligibilityStatus status, String benefitDetails, LocalDateTime verifiedAt) {
		super();
		this.eligibilityId = eligibilityId;
		this.planName = planName;
		this.planProvider = planProvider;
		this.planType = planType;
		this.planCost = planCost;
		this.status = status;
		this.benefitDetails = benefitDetails;
		this.verifiedAt = verifiedAt;
	}

	public String getEligibilityId() {
		return eligibilityId;
	}

	public void setEligibilityId(String eligibilityId) {
		this.eligibilityId = eligibilityId;
	}

	public String getPlanName() {
		return planName;
	}

	public void setPlanName(String planName) {
		this.planName = planName;
	}

	public String getPlanProvider() {
		return planProvider;
	}

	public void setPlanProvider(String planProvider) {
		this.planProvider = planProvider;
	}

	public String getPlanType() {
		return planType;
	}

	public void setPlanType(String planType) {
		this.planType = planType;
	}

	public Integer getPlanCost() {
		return planCost;
	}

	public void setPlanCost(Integer planCost) {
		this.planCost = planCost;
	}

	public EligibilityStatus getStatus() {
		return status;
	}

	public void setStatus(EligibilityStatus status) {
		this.status = status;
	}

	public String getBenefitDetails() {
		return benefitDetails;
	}

	public void setBenefitDetails(String benefitDetails) {
		this.benefitDetails = benefitDetails;
	}

	public LocalDateTime getVerifiedAt() {
		return verifiedAt;
	}

	public void setVerifiedAt(LocalDateTime verifiedAt) {
		this.verifiedAt = verifiedAt;
	}
    
    
}
